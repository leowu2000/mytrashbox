<?xml version="1.0" encoding="UTF-8"?>
<config>
	<head>
		<table>ASSETS_INFO_REPAIR</table>
		<code>ASSETS_INFO_REPAIR</code>
		<name>信息设备维修记录导入</name>
		<row>2</row>
		<col>1</col>
		<type>1</type>
		<count></count>
		<split></split>
	</head>
	<body>
		<relation>
			<id>1</id>
			<name>资产编号</name>
			<column>CODE</column>
			<format></format>
		</relation>
		<relation>
			<id>2</id>
			<name>设备名称</name>
			<column>NAME</column>
			<format></format>
		</relation>
		<relation>
			<id>3</id>
			<name>维修日期</name>
			<column>R_DATE</column>
			<format>yyyy-MM-dd</format>
		</relation>
		<relation>
			<id>4</id>
			<name>维修费用</name>
			<column>R_COST</column>
			<format></format>
		</relation>
		<relation>
			<id>5</id>
			<name>维修原因</name>
			<column>R_REASON</column>
			<format></format>
		</relation>
		<relation>
			<id>6</id>
			<name>维修情况</name>
			<column>R_NOTE</column>
			<format></format>
		</relation>
	</body>
</config>