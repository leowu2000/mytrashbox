<?xml version="1.0" encoding="UTF-8"?>
<config>
	<head>
		<table>ASSETS</table>
		<code>ASSETS</code>
		<name>固定资产表</name>
		<row>2</row>
		<col>1</col>
		<type>1</type>
		<count></count>
		<split></split>
	</head>
	<body>
		<relation>
			<id>1</id>
			<name>设备编号</name>
			<column>CODE</column>
			<format></format>
		</relation>
		<relation>
			<id>2</id>
			<name>设备名称</name>
			<column>NAME</column>
			<format></format>
		</relation>
		<relation>
			<id>3</id>
			<name>设备型号</name>
			<column>MODEL</column>
			<format></format>
		</relation>
		<relation>
			<id>4</id>
			<name>购买日期</name>
			<column>BUYDATE</column>
			<format>yyyy-MM-dd</format>
		</relation>
		<relation>
			<id>5</id>
			<name>出厂日期</name>
			<column>PRODUCEDATE</column>
			<format>yyyy-MM-dd</format>
		</relation>
		<relation>
			<id>6</id>
			<name>使用年限</name>
			<column>LIFE</column>
			<format></format>
		</relation>
		<relation>
			<id>7</id>
			<name>购买价格</name>
			<column>BUYCOST</column>
			<format></format>
		</relation>
		<relation>
			<id>8</id>
			<name>状态</name>
			<column>STATUS</column>
			<format></format>
		</relation>
		<relation>
			<id>9</id>
			<name>领用人</name>
			<column>EMPNAME</column>
			<format></format>
		</relation>
		<relation>
			<id>10</id>
			<name>领用时间</name>
			<column>LENDDATE</column>
			<format>yyyy-MM-dd</format>
		</relation>
		<relation>
			<id>11</id>
			<name>上次年检时间</name>
			<column>CHECKDATE</column>
			<format>yyyy-MM-dd</format>
		</relation>
		<relation>
			<id>12</id>
			<name>年检间隔（年）</name>
			<column>CHECKYEAR</column>
			<format></format>
		</relation>
	</body>
</config>